{
    "name": "interiete/moreglue",
    "type": "library",
    "description": "Micro framework",
    "keywords": [
        "doctrine2",
        "twig",
        "framework"
    ],
    "homepage": "http://github.com/LeisureLarry/moreglue",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Teriete",
            "email": "leisure.larry@habmalnefrage.de",
            "homepage": "http://blog-das-oertchen.de/"
        }
    ],
    "require": {
        "php":">=5.3.7",
        "symfony/http-foundation": "~2.2.0",
        "symfony/routing": "~2.2.0",
        "symfony/http-kernel": "~2.2.0",
        "mnapoli/php-di": "~2.1.0",
        "monolog/monolog": "~1.4.0",
        "doctrine/orm":"~2.3.2",
        "doctrine/doctrine-bundle": "v1.2.0-beta1",
        "doctrine/migrations": "v1.0.0-ALPHA1",
        "doctrine/data-fixtures": "v1.0.0-ALPHA3",
        "webmasters/doctrine-extensions": "~2.3.4",
        "Respect/Validation": "~0.4.4",
        "ircmaxell/password-compat": "~1.0.0",
        "twig/twig": "~1.12.2",
        "symfony/twig-bridge": "~2.2.0"
    },
    "autoload": {
        "psr-0": { "MoreGlue": "lib/" }
    },
    "minimum-stability": "alpha",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
